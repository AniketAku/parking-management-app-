{
  "name": "parking-management-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:deploy": "vite build --mode production",
    "build:performance": "tsc -b && vite build --config vite.config.performance.ts",
    "build:analyze": "ANALYZE=true npm run build:performance",
    "build:report": "npm run build:performance && node scripts/build-optimization.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "ANALYZE=true vite build && npx serve dist",
    "bundle:analyze": "node scripts/build-optimization.js",
    "optimize": "npm run build:performance && npm run bundle:analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --run src/test/integration",
    "test:accessibility": "vitest --run src/test/accessibility",
    "test:pwa": "vitest --run src/test/pwa",
    "test:components": "vitest --run src/components",
    "test:all": "npm run test:components && npm run test:integration && npm run test:accessibility && npm run test:pwa",
    "test:performance": "vitest --run src/test/performance"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.1.4",
    "@hookform/resolvers": "^3.6.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.45.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "export-to-csv": "^1.4.0",
    "framer-motion": "^11.2.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.52.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.23.1",
    "react-to-print": "^3.1.1",
    "react-window": "^1.8.8",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.10.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^10.3.10",
    "globals": "^16.3.0",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^1.6.1",
    "workbox-window": "^7.1.0"
  }
}
